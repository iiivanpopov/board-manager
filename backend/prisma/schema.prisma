generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Record {
  id          Int      @id @default(autoincrement())
  inspector   String
  product     String
  problemType String
  problem     String
  createdAt   DateTime @default(now())
}
