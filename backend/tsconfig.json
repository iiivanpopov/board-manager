{
	"compilerOptions": {
		"target": "es6",
		"module": "ESNext",
		"outDir": "./dist",
		"strict": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"moduleResolution": "Bundler",
		"allowImportingTsExtensions": true,
		"noEmit": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
